$(document).ready(function() {

  $('#logo').click(function(){
    $('#nav').toggle();
    if ($('#vol').is(':visible')) {
      $('#vol').hide();
    }
  });

  $('#openvol').click(function(){
    $('#vol').toggle();
  });

  $('#very').click(function(){
    if ($('#nav').is(':visible')) {
      $('#nav').hide();
    }
  });

});